{
  "name": "hybrid-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "deploy": "jkupload -s dist -p hybrid-template && gulp upload",
    "publish": "npm run build && npm run deploy",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "vue": "^2.6.9",
    "vue-router": "^3.0.2",
    "@ruguoapp/jike-auth-axios": "0.0.4",
    "@ruguoapp/jike-js-sdk": "0.0.30",
    "@ruguoapp/jk-analytics": "0.3.1",
    "mint-ui": "^2.2.13",
    "@ruguoapp/vue-jike-navigation": "^0.3.1",
    "await-to-js": "^2.1.1",
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "shelljs": "^0.8.3",
    "gulp": "^4.0.0",
    "@ruguoapp/jike-fe-upload": "0.0.15",
    "@vue/cli-plugin-babel": "^3.5.1",
    "@vue/cli-plugin-eslint": "^3.5.1",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "@vue/cli-service": "^3.5.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.15.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "eslint-plugin-standard": "^4.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.6.9"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
